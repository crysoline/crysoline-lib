{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "lib": ["ESNext"],
    "rootDir": "src",
    "outDir": "dist",
    "declaration": true,
    "moduleResolution": "bundler",
    "verbatimModuleSyntax": true,
    "strict": true,
    "skipLibCheck": true,
  },
  "include": ["src/**/*"],
}
